pid: /vagrant/Python/ProcessMaster/tests/daemons/run/workers-test.pid
user: vagrant
expect: daemon

respawn:
    limit: 5
    interval: 5

signals:
    kill: 15
    reload: 10

timeouts:
    start: 2
    stop: 5

run: /vagrant/Python/ProcessMaster/tests/daemons/workers.py start
stop: /vagrant/Python/ProcessMaster/tests/daemons/workers.py stop


